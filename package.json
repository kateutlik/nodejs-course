{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.ts",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "start": "nodemon src/server.ts",
    "tsc": "tsc",
    "lint": "eslint ./ --ignore-path .gitignore --fix",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=12.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "commander": "^5.0.0",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "http-status": "^1.4.2",
    "joi": "^14.3.1",
    "mongoose": "^5.9.9",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.1-alpha.2",
    "swagger-ui-express": "4.1.2",
    "ts-node": "^8.8.2",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.8.3",
    "uuid": "3.3.3",
    "winston": "^3.2.1",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.4",
    "@types/mongoose": "^5.7.12",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^7.0.2",
    "@types/winston": "^2.3.7",
    "@types/yamljs": "^0.2.30",
    "chai": "4.2.0",
    "debug": "4.1.1",
    "dirty-chai": "2.0.1",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "supertest": "4.0.2"
  }
}
